{
  "name": "cicr-connect-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": ">=22.22.0 <23"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm install",
    "lint": "node scripts/lint.js",
    "test": "node --test tests/**/*.test.js",
    "test:coverage": "node --test --experimental-test-coverage tests/**/*.test.js",
    "check:migrations": "node scripts/checkMigrations.js",
    "migrate:encrypt": "node scripts/migrateEncryptExistingData.js",
    "migrate:encrypt:dry-run": "DRY_RUN=true node scripts/migrateEncryptExistingData.js",
    "verify:encryption": "node scripts/verifyEncryptedData.js",
    "verify:encryption:fix": "FIX_HASHES=true node scripts/verifyEncryptedData.js",
    "perf:smoke": "node scripts/perfSmoke.js",
    "audit:prod": "npm audit --omit=dev --audit-level=high"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.0",
    "nodemailer": "^7.0.12"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
