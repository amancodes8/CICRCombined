PORT=4000
NODE_ENV=development
MONGO_URI=mongodb+srv://<user>:<password>@<cluster>/<db>?retryWrites=true&w=majority
JWT_SECRET=replace-with-long-random-secret
JWT_EXPIRES_IN=30d
JWT_ISSUER=cicr-connect
JWT_AUDIENCE=cicr-connect-client
DATA_ENCRYPTION_KEY=replace-with-strong-32-byte-secret-or-base64
# Optional: comma-separated previous keys used to encrypt/hash old records
DATA_ENCRYPTION_OLD_KEYS=
FRONTEND_URL=http://localhost:5173
AUTH_MAX_FAILED_ATTEMPTS=6
AUTH_LOCK_MINUTES=20
REQUEST_BODY_LIMIT_KB=200
HEALTH_DETAIL_ENABLED=false

# Resend email provider
RESEND_API_KEY=re_xxxxxxxxxxxxxxxxx
RESEND_FROM_EMAIL=CICR Connect <noreply@yourdomain.com>

# Optional AI integration
GEMINI_API_KEY=
